{
  "expo": {
    "scheme": "ecmidapp",
    "name": "eCMIDApp",
    "slug": "eCMIDApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/logo_light.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.eCMIDApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/img/logo_light.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1e9fd8"
        }
      ]
    ]
  }
}