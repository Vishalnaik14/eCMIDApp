{
  "expo": {
    "scheme": "ecmidapp",
    "name": "eCMIDApp",
    "slug": "eCMIDApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/ecmidlogoblack.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/img/ecmidlogoblack.png",
      "resizeMode": "contain",
      "backgroundColor": "#1e9fd8"
    },
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": false,
      "statusBarStyle": "light-content",
      "statusBarBackgroundColor": "#1e9fd8",
      "splash": {
        "image": "./assets/img/ecmidlogoblack.png",
        "resizeMode": "contain",
        "backgroundColor": "#1e9fd8"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/ecmidlogoblack.png",
        "backgroundColor": "#1e9fd8"
      },
      "splash": {
        "image": "./assets/img/ecmidlogoblack.png",
        "resizeMode": "contain",
        "backgroundColor": "#1e9fd8"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.eCMIDApp",
      "statusBarStyle": "light-content",
      "statusBarBackgroundColor": "#1e9fd8"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/img/ecmidlogoblack.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1e9fd8"
        }
      ]
    ]
  }
}